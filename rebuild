#!/bin/sh
export CMAKE_PREFIX_PATH=/Users/francis/Qt5.6.2/5.6/clang_64/lib/cmake:/Users/francis/Qt5.6.2/5.6/clang_64/lib/cmake/Qt5:/Users/francis/Qt5.6.2/5.6/clang_64/lib/cmake/Qt5Gui
rm -rf build && mkdir build
(cd build && cmake .. && make -j8)

[ -f build/QtTetris/QtTetris ] && {
    echo "Build succeeded: build/QtTetris/QtTetris"
}
